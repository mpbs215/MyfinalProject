<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="faqMapper">
	<insert id="insertFAQ" parameterType="fAQDTO">
		insert into faq_tb
		values(terms_seq.nextval,#{FAQSub},#{FAQContent})
	</insert>

	<select id="updateFormFAQ" parameterType="int"
		resultType="faqDTO">
		select * from faq_TB
		where faq_no=#{FAQNo}
	</select>

	<update id="updateFAQ" parameterType="faqDTO">
		update faq_tb set
		faq_sub=#{FAQSub},faq_content=#{FAQContent}
		where faq_no=#{FAQNo}
	</update>

	<delete id="deleteFAQ" parameterType="int">
		delete from faq_TB
		where
		faq_no=#{FAQNo}
	</delete>

</mapper>